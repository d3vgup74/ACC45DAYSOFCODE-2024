class Solution(object):
    def longestCommonPrefix(self, strs):
        common = ""
        for i in range(len(strs[0])):
            for j in range(1, len(strs)):
                flag = True

                try: 
                    if strs[0][i] != strs[j][i]:
                        flag = False
                        break
                except IndexError:
                    common = strs[j]
                    flag = False
                
            if (flag):
                common += strs[0][i]
                
        return common

ip = input()
ip = ip.split(" ")

ques = Solution()
ans = ques.longestCommonPrefix(ip)
print(ans)
